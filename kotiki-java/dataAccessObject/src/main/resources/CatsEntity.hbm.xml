<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.itmo.kotiki.entity.CatsEntity" table="cats" schema="public" catalog="kotiki">
        <id name="catId">
            <column name="cat_id" sql-type="integer"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar" not-null="true"/>
        </property>
        <property name="birthday">
            <column name="birthday" sql-type="date"/>
        </property>
        <property name="color">
            <column name="color" sql-type="varchar" not-null="true"/>
        </property>
        <property name="breed">
            <column name="breed" sql-type="varchar" not-null="true"/>
        </property>
        <many-to-one name="humanByHumansId" class="com.itmo.kotiki.entity.HumansEntity">
            <column name="human_id" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>